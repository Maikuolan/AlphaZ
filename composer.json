{
    "name": "alphaz/framework",
    "description": "The AlphaZ Framework.",
    "keywords": ["framework", "alphaz"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "support": {
        "issues": "https://github.com/alphazframework/framework/issues",
        "source": "https://github.com/alphazframework/framework"
    },
    "authors": [
        {
            "name": "Muhamamad Umer Farooq",
            "email":"umer@lablnet.com",
            "homepage": "https://lablnet.com/"
        }
    ],
    "autoload": {
        "psr-4": {
             "alphaz\\": "src/"
        },
        "files":["src/functions/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Framework\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config":{
        "optimize-autoloader":true
    },
    "prefer-stable": true
}
